# .env (Frontend)

# API Base URL
REACT_APP_API_URL=http://localhost:5000/api

# Environment
REACT_APP_ENV=development

# Feature flags
REACT_APP_FEATURE_X=true

# Third-party keys (example)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
REACT_APP_ANALYTICS_KEY=your_analytics_key_here

# Optional: Frontend port
PORT=3000
